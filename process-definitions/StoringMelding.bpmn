    </sequenceFlow>
    <sequenceFlow id="flow10" name="Start" sourceRef="start" targetRef="scripttask1"></sequenceFlow>
    <scriptTask id="scripttask1" name="Script Task" scriptFormat="groovy" activiti:autoStoreVariables="true">
      <script>println "TADA! This is a script!"</script>
    </scriptTask>
    <sequenceFlow id="flow11" sourceRef="scripttask1" targetRef="afhandelen"></sequenceFlow>
